{
  "expo": {
    "plugins": [
      [
        "expo-notifications",
        {
          "icon": "./assets/img/icon.png",
          "color": "#ffffff",
          "defaultChannel": "default",
          "sounds": [
            "./assets/sounds/default.mp3"
          ]
        },
        "expo-av",
        {
          "microphonePermission": "Allow $(healthCare) to access your microphone."
        }
      ]
    ],
    "assetBundlePatterns": [
      "assets/**/*"
    ],
    "name": "healthCare",
    "slug": "healthCare",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/img/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/img/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "ios": {
      "supportsTablet": true,
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/img/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      }
    },
    "web": {
      "favicon": "./assets/img/favicon.png"
    }
  }
}
